{
  "$schema": "https://json-schema.boschrexroth.com/ctrlx-automation/ctrlx-core/apps/package-manifest/package-manifest.v1.4.schema.json",
  "version": "1.0.0",
  "id": "sdk-py-location-transform",
  "services": {
    "proxyMapping": [
      {
        "name": "sdk-py-location-transform.web",
        "url": "/sdk-py-location-transform",
        "binding": "unix://{$SNAP_DATA}/package-run/sdk-py-location-transform/web.sock",
        "restricted": [
          "/sdk-py-location-transform/api/v1"
        ]
      }
    ]
  },
  "commands": {
    "activeConfiguration": {
      "load": [
        {
          "id": "sdk-py-location-transform",
          "subject": "location-transform.py",
          "url": "https://localhost/sdk-py-location/api/v1/load"
        }
      ],
      "save": [
        {
          "id": "sdk-py-location-transform",
          "subject": "location-transform.py",
          "url": "https://localhost/sdk-py-appdata/api/v1/save"
        }
      ]
    }
  },
  "scopes-declaration": [
    {
      "identifier": "sdk-py-location-transform.web",
      "name": "sdk-py-location-transform",
      "description": "sdk-py-location-transform permissions",
      "scopes": [
        {
          "identifier": "sdk-py-location-transform.web.rwx",
          "name": "Write Location Transform",
          "description": ""
        },
        {
          "identifier": "sdk-py-location-transform.web.r",
          "name": "View Location Trasform",
          "description": ""
        }
      ]
    }
  ],
  "configuration": {
    "appDirectories": [
      {
        "name": "LocationTransform",
        "description": "Location Transform from Pixel to Robot Space",
        "icon": "bosch-ic-directory",
        "copyOnLoad": true
      }
    ]
  }
}